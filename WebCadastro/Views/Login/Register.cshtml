@{
    Layout = null;
}
<script src="~/Scripts/knockout-3.5.1.js"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>


<div class="container text-center m-auto">
    <div class="container body-content d-flex justify-content-center gap-2">
        <div class="d-flex flex-column gap-4 col-sm-8 col-md-5 col-lg-7 justify-content-center align-items-center flex-wrap shadow p-3 mb-5 bg-body-tertiary rounded" style="background-color:lightgray">
            <h3>Cadastro</h3>
            <div class="d-flex flex-row gap-3 m-auto col-12 justify-content-center">
                <label class="col-sm-2 col-form-label" for="Nome">Nome:*</label>
                <input id="Nome" class="form-control" type="text" placeholder="Nome" data-bind="value: Nome, click: reset" />
            </div>
            <div class="d-flex flex-row gap-3 m-auto col-12 justify-content-center">
                <label class="col-sm-2 col-form-label" for="Nome">Email:*</label>
                <input id="Email" class="form-control" type="text" placeholder="Email" data-bind="value: Email, click: reset" />
            </div>
            <div class="d-flex flex-row gap-3 m-auto col-12 justify-content-center">
                <label class="col-sm-2 col-form-label" for="Nome">Senha:*</label>
                <input id="Senha" class="form-control" type="text" placeholder="Senha" data-bind="value: Senha, click: reset" />
            </div>
            <div class="d-flex flex-row gap-3 m-auto col-12 justify-content-center">
                <label class="col-sm-2 col-form-label" for="Salario">Salário:</label>
                <input id="Salario" class="form-control" type="text" placeholder="Salário" data-bind="value: Salario" />
            </div>
            <div class="d-flex flex-row gap-3 m-auto col-12 justify-content-center">
                <label class="col-sm-2 col-form-label" for="Nome">Limite:</label>
                <input id="Limite" class="form-control" type="text" placeholder="Limite" data-bind="value: Limite" />
            </div>
            <div class="d-flex flex-row gap-3 m-auto col-12 justify-content-center">
                <label class="col-sm-2 col-form-label" for="Minimo">Mínimo:*</label>
                <input id="Minimo" class="form-control" type="text" placeholder="Mínimo" data-bind="value: Minimo, click: reset" />
            </div>
            <div class="d-flex flex-row gap-3 m-auto col-10 justify-content-center">
                <a href="/Login/Index" class="text-decoration-none">Login</a>
            </div>
            <button class="btn btn-primary" data-bind="click: Send.bind(Nome)">Cadastrar</button>
        </div>
    </div>
</div>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/knockout-3.5.1.js"></script>
@Styles.Render("~/Content/css")
@Scripts.Render("~/bundles/modernizr")
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/Login/Register")
